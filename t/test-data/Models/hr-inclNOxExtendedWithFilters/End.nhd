+Summary
  type = report
?if Kantonal_LU
  submit = true
   ++selector
     en = Berechnung Baugesuch Kanton Luzern - Ammonia emissions (in kg N per year)
     de = Berechnung Baugesuch Kanton Luzern - Ammoniak-Emissionen (in kg N pro Jahr)
     fr = Baugesuch Kanton Luzern - Emissions d'ammoniaque (en kg de N par année)
?else
  ++selector
    en = Ammonia emissions (in kg N per year) - summary
    de = Ammoniak-Emissionen (in kg N pro Jahr) - Zusammenfassung
    fr = Emissions d'ammoniaque (en kg de N par année) - résumé
?endif
  ++data
    +++SummaryLivestock
      en = Livestock
      de = Tierproduktion
      fr = Production animale
    +++SummaryPlantProduction
      en = Plant production
      de = Pflanzenbau
      fr = Production végétale
    +++SummaryTotal
      en = Total
      de = Total
      fr = Total

+SummaryShort
  type = report
  results = true
  ++selector
    en = Ammonia emissions (in kg N per year) - summary short
    de = Ammoniak-Emissionen (in kg N pro Jahr) - Kurzzusammenfassung
    fr = Emissions d'ammoniaque (en kg de N par année) - résumé minimal
  ++data
     +++SummaryTotal
       en = Total
       de = Total
       fr = Total

?if !KantonalLU
  +DetailReport
    type = reportDetailed
    ++selector
      en = Emissions and fluxes (in kg N per year) - detailed report
      de = Emissionen und Flüsse (in kg N pro Jahr) - Detaillreport
      fr = Emissions et flux d'azote (en kg de N par année) - résumé detaillé
    ++data
       +++LivestockNH3
         en = Livestock production NH3-Emissions
         de = Tierproduktion NH3-Emissionen
         fr = Production animale Emissions de NH3 
       +++PlantNH3
         en = Plant production NH3-Emissions
         de = Pflanzenbau NH3-Emissionen
         fr = Production végétale Emissions de NH3
      +++SummaryTotal
        en = Total NH3-Emissions
        de = Total NH3-Emissionen
        fr = Totales Emissions de NH3 
      +++LivestockN2O
        en = Livestock production N2O-Emissions
        de = Tierproduktion N2O-Emissionen
        fr = Production animale Emissions de N2O
      +++LivestockNO
        en = Livestock production NO-Emissions
        de = Tierproduktion NO-Emissionen
        fr = Production animale Emissions de NO
      +++LivestockN2
        en = Livestock production N2-Emissions
        de = Tierproduktion N2-Emissionen
        fr = Production animale Emissions de N2
      +++LivestockNtot
        en = Nitrogen flux livestock production
        de = Stickstofffluss Tierproduktion
        fr = Flux d'azote production animale
      +++LivestockTAN
        en = TAN flux livestock production
        de = TAN Fluss Tierproduktion
        fr = Flux de TAN production animale
?endif


+SummaryBarGraph
   type = bar
   ++selector
     en = Ammonia emissions (in kg N per year) - summary (bar chart)
     de = Ammoniak Emissionen (in kg N pro Jahr) - Zusammenfassung (Balkengrafik)
     fr = Emissions d'ammoniaque (en kg de N par année) - résumé (diagramme à barres)
   ++data
     +++SummaryLivestock
       en = Livestock
       de = Tierproduktion
       fr = Production animale
     +++SummaryPlantProduction
       en = Plant production
       de = Pflanzenbau
       fr = Production

+SummaryPieGraph
   type = pie
   ++selector
     en = Ammonia emissions (in kg N per year) - summary (pie chart)
     de = Ammoniak Emissionen (in kg N pro Jahr) - Zusammenfassung (Kuchendiagramm)
     fr = Emissions d'ammoniaque (en kg de N par année) - résumé (diagramme en camembert)
   ++data
     +++SummaryLivestock
       en = Livestock
       de = Tierproduktion
       fr = Production animale
     +++SummaryPlantProduction
       en = Plant production
       de = Pflanzenbau
       fr = Production

