      }
      when 'Loose_Housing_Slurry_Plus_Solid_Manure' {
        return Val(er_housing, Type::Loose_Housing_Slurry_Plus_Solid_Manure);
      }
      when 'Loose_Housing_Deep_Litter' {
        return Val(er_housing, Type::Loose_Housing_Deep_Litter);
      }
    }

